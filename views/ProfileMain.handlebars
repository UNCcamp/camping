            <div class="row">
              {{#each this}}
              <div class="col-md-3">
                <a href="#" class="thumbnail"><img src="{{this}}" alt="Image" style="max-width:100%;"></a>
              </div>
              {{/each}}
            </div>
            <!--.row-->
          </div>
          <!--.item-->

          <!--.item-->
            <!--.row-->
            {{/each}}
          </div>
        <!--.carousel-inner-->
        <a data-slide="prev" href="#Carousel3" class="left carousel-control">‹</a>
        <a data-slide="next" href="#Carousel3" class="right carousel-control">›</a>
      </div>
      <!--.Carousel-->
    </div>
  </div>
