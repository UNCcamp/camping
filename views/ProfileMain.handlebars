            <li data-target="#Carousel2" data-slide-to="1"></li>
            <li data-target="#Carousel2" data-slide-to="2"></li>
          </ol>
          <!-- Carousel items -->
          <div class="carousel-inner">
            {{#each campImages}} <!-- trail images -->
            <div class="item active">
              <div class="row">
                {{#each this}}
